    </servlet>
    <servlet>
        <servlet-name>srvSocio</servlet-name>
        <servlet-class>servlet.srvSocio</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>srvBanco</servlet-name>
        <servlet-class>servlet.srvBanco</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>srvLstBancos</servlet-name>
        <servlet-class>servlet.srvLstBancos</servlet-class>
    </servlet>

	<servlet>
        <servlet-name>srvUsuario</servlet-name>
        <servlet-class>servlet.srvUsuario</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>srvGenero</servlet-name>
        <url-pattern>/srvGenero</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>srvAlquiler</servlet-name>
        <url-pattern>/srvAlquiler</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>srvLineaAlquiler</servlet-name>
        <url-pattern>/srvLineaAlquiler</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>srvLogin</servlet-name>
        <url-pattern>/srvLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>srvLstUsuarios</servlet-name>
        <url-pattern>/srvLstUsuarios</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>srvPelicula</servlet-name>
        <url-pattern>/srvPelicula</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>srvSocio</servlet-name>
        <url-pattern>/srvSocio</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>srvBanco</servlet-name>
        <url-pattern>/srvBanco</url-pattern>
