    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>  
  
   <servlet>
        <servlet-name>srvGenero</servlet-name>
        <servlet-class>servlet.srvGenero</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>srvAlquiler</servlet-name>
        <servlet-class>servlet.srvAlquiler</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>srvLineaAlquiler</servlet-name>
        <servlet-class>servlet.srvLineaAlquiler</servlet-class>
    </servlet>
    <servlet>
